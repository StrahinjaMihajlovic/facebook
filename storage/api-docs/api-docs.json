    "paths": {
        "/api/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Create new user",
                "operationId": "register",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password"
                                    }
                                },
                                "type": "object"
                            },
                            "example": {
                                "name": "Marko",
                                "email": "marko@mail.com",
                                "password": "1234ABCDEF",
                                "password_confirmation": "1234ABCDEF"
                            }
                        }
                    }
                },
                "responses": {
                    "422": {
                        "description": "Unprocessable Entity",
                        "content": {
                            "application/json": {
                                "example": {
                                    "message": "The given data was invalid.",
                                    "errors": "All fields is required."
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "example": {
                                    "message": "Data into fields is invalid."
                                }
                            }
                        }
                    },
                    "200": {
                        "description": "New user successfully register",
                        "content": {
                            "application/json": {
                                "example": {
                                    "name": "Mike",
                                    "email": "Mike@mail.com",
